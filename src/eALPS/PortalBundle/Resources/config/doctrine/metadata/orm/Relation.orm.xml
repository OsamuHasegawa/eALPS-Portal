<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Relation" table="Relation">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="enable" type="boolean" column="enable" nullable="false"/>
    <field name="original" type="boolean" column="original" nullable="false"/>
    <field name="createdat" type="datetime" column="createdAt" nullable="false"/>
    <field name="updatedat" type="datetime" column="updatedAt" nullable="false"/>
    <many-to-one field="relationRole" target-entity="RelationRole">
      <join-columns>
        <join-column name="relation_role_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="account" target-entity="Account">
      <join-columns>
        <join-column name="account_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="course" target-entity="Course">
      <join-columns>
        <join-column name="course_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
