<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="BatchResult" table="Batch_Result">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="message" type="text" column="message" nullable="true"/>
    <field name="startedat" type="datetime" column="startedAt" nullable="true"/>
    <field name="finishedat" type="datetime" column="finishedAt" nullable="true"/>
    <field name="trigger" type="string" column="trigger" length="255" nullable="true"/>
  </entity>
</doctrine-mapping>
